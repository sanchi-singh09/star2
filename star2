#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n;
    scanf("%d",&n);
    for(int i=1; i<=n; i++)
    {
        for(int j=i; j<=n; j++)
        {
            printf("%d",j);
        }
        for(int j=1; j<i; j++)
        {         
                printf("%d",n);

        }
        printf("\n");
    }
    return 0;
}
